# input=geeksforgeeks

test_str = input("enter string: ")

count = 0

for i in test_str:
	if i == 'e':
		count = count + 1

print("Count of e in GeeksforGeeks is : "
	+ str(count))


# input= "abccdefgaa"

test_str = input("enter string: ")

count = 0

for i in test_str:
	if i == 'a':
		count = count + 1


print("Count of a in GeeksforGeeks is : "
	+ str(count))
